{
  "name": "open3d",
  "version": "0.2.2",
  "description": "Open3d is a 3d geometry library for javascript.",
  "main": "./dist/open3d.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/ccc159/open3d",
  "scripts": {
    "doc": "typedoc&&serve docs -l 8081",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "format": "eslint src/**/*.ts test/**/*.ts --fix",
    "style": "prettier --check src/**/*.ts test/**/*.ts",
    "build": "webpack&&typedoc",
    "test": "jest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint & yarn style",
      "pre-push": "yarn test"
    }
  },
  "keywords": [
    "geometry",
    "3d",
    "vector"
  ],
  "author": {
    "name": "Jingcheng Chen",
    "email": "mail@chenjingcheng.com",
    "url": "https://chen.works/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccc159/open3d.git"
  },
  "bugs": {
    "url": "https://github.com/ccc159/open3d/issues",
    "email": "mail@chenjingcheng.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-jest": "^27.1.6",
    "husky": "^8.0.2",
    "jest": "^28.1.1",
    "prettier": "^2.8.0",
    "serve": "^13.0.2",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
